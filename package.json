{
  "name": "nest-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prettier": "prettier --write './src/**/*.ts'",
    "lint": "eslint './src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:graphql": "jest --config ./test/jest-graphql.json",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration": "yarn typeorm --config src/database/configs/migration.config.ts",
    "migration:create": "yarn typeorm:migration migration:create --dir src/database/migrations -n",
    "migration:run": "yarn typeorm:migration migration:run",
    "migration:revert": "yarn typeorm:migration migration:revert",
    "typeorm:seed": "yarn typeorm --config src/database/configs/seed.config.ts",
    "seed:create": "yarn typeorm:seed migration:create --dir src/database/seeds -n",
    "seed:run": "yarn typeorm:seed migration:run",
    "seed:revert": "yarn typeorm:seed migration:revert",
    "docs": "compodoc --tsconfig tsconfig.json --serve"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/typeorm": "^8.0.1",
    "apollo-server-express": "^3.0.2",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "joi": "^17.4.1",
    "mongoose": "^6.1.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.14",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts, tsx}": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  }
}
